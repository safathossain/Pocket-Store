<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appuitestbench">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".purchaselist_without_mic"></activity>
        <activity android:name=".call_starting_window" />
        <activity android:name=".purchase_list" />
        <activity android:name=".setup_profile_shopOwner" />
        <activity android:name=".RegisterNumberActivity" />
        <activity android:name=".GetStartedActivity" />
        <activity
            android:name=".SetupProfileActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".VerificationPageActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".MenuActivity" />
        <activity
            android:name=".LoginActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>